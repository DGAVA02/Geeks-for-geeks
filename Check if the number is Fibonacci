    def checkFibonacci (ob,N):
        # code here
        n1=0
        n2=1
        l=[]
        count=0
        if N==1 :
            return "Yes"
        while count<=100:
            l.append(n1)
            nth=n1+n2
            n1=n2
            n2=nth
            count=count+1
        if N in l:
            return "Yes"
        else :
            return "No"
